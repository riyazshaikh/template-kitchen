<!doctype html>
<html class="touch-styles">
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="initial-scale=1">

    {@|apply js-utils.block}

    <!--Include sqs scripts, meta tags, and user content from the code injection tab.-->
    {squarespace-headers}

     <squarespace:script src="ElementResize.js" />    
    <squarespace:script src="ElementQueries.js" />    
    <squarespace:script src="rafscroll.js" />    

    <script>/* Must be below {squarespace-headers} */(function(){var e='ontouchstart'in window||navigator.msMaxTouchPoints;var t=document.documentElement;if(!e&&t){t.className=t.className.replace(/touch-styles/,'')}})()
    </script>
  </head>
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">

    <div class="sqs-announcement-bar-dropzone"></div>

    <div id="site" data-region="{collection.regionName}">

      <div id="header" role="banner">
        {@|apply header.block}
      </div>

      <div id="banner" role="banner">
        {collection|apply banner.block}
      </div>

      <main id="{collection.typeName}{.if item}-item{.end}" role="main" data-palette="main" data-typeset="main" data-content-field="main-content">
        {@|apply nav-category.block}
        <div class="content" data-layout-width="main">
          {squarespace.main-content}
        </div>
      </main>

      <div id="page-footer" role="complementary">
        {collection|apply page-footer.block}
      </div>

      <footer id="footer" role="contentinfo">
        {@|apply footer.block}
      </footer>
    </div>

    <script>
      SquareMart.RecipeManager.force(); // make sure dom is ready for scripts to use
    </script>


    <!--Combo and minify the scripts when you're not logged in.-->
    <squarespace:script src="helpers.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />
    <squarespace:script src="sticky.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />
    <squarespace:script src="parallax.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />
    <squarespace:script src="auto-palette.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />
    <script>
    // check for old browsers - https://browser-update.org/
    function $buo_f(){var e=document.createElement("script");e.src="//browser-update.org/update.min.js",document.body.appendChild(e)}var $buoop={vs:{i:10,f:35,o:25,s:7},c:2};try{document.addEventListener("DOMContentLoaded",$buo_f,!1)}catch(e){window.attachEvent("onload",$buo_f)}
    </script>    

    <!--Injection point for tracking scripts and user content from the code injection tab.-->
    {squarespace-footers}

    {.if authenticatedAccount}
    <squarespace:script src="tweaking.js" />
    {.end}
  </body>
</html> 
