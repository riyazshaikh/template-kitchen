{
  "title" : "Default",
  "templateTweaks" : {
			"header---display": "Overlaid",
			"header---background": "Transparent",
			"header---nav": "Slidebar",
			"header--navigation---align": "Center",
			"header--content---layout-width": "Main",

	    "banner---palette": "Alternate",
	    "footer---palette": "Main",
			"metaWidth": "47%",

			"body-font": "{font-family:'Open Sans','Helvetica Neue',Helvetica,Arial,sans-serif;font-size:20px;font-weight:300;line-height:1.5em;}",
			"heading2-font": "{font-family:inherit;font-size:36px;font-weight:700;line-height:1.2em;}",

			"banner-body-font": "{font-family:'Open Sans','Helvetica Neue',Helvetica,Arial,sans-serif;font-size:24px;font-weight:300;line-height:1.3em;}",
			"banner-heading1-font": "{font-family:inherit;font-size:40px;font-weight:700;line-height:1.2em;}",

			"main-bgColor": "#f3efee",
			"main-metaColor": "rgba(15,15,15,0.5)"
	},
	"recipes": {
			"#banner .description": "display=overlaid-caption",
			"#banner .description .content": ["layout-spacing=sides","align=center-left-parent"],
			"#banner .description .content > div": ["layout-width=meta","layout-spacing=none"],
			"#banner .blocks .content": ["layout-width=main","layout-spacing=sides","parallax=enable"],
			"#banner .description .title": "appearance=meta",
			"#banner .description strong": ["appearance=heading1","font-effect=dash"],
			"#banner .description a": "appearance=button-solid-pill",

			"#page .content": ["layout-width=full","layout-spacing=regular"],
			"#page .content h2": "font-effect=dash",
	    "#page": "palette=alternate"
	}
}